{
    "name": "divineomega/uxdm",
    "description": "UXDM helps developers migrate data from one system or format to another.",
    "type": "library",
    "authors": [
        {
            "name": "Jordan Hall",
            "email": "jordan@hall05.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "DivineOmega\\uxdm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DivineOmega\\uxdm\\TestClasses\\": "tests/Unit/includes/TestClasses/"
        }
    },
    "license": "LGPL-3.0-only",
    "require": {
        "php": ">=7.1",
        "ext-pdo": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "psr/cache": "^1.0",
        "divineomega/array_undot": "^2.0",
        "divineomega/php-cli-progress-bar": "^2.0",
        "divineomega/omega-validator": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0||^8.0",
        "fzaninotto/faker": "^1.6",
        "php-coveralls/php-coveralls": "^2.0",
        "cache/array-adapter": "^1.0"
    }
}
